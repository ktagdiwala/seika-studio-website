<%- include("partials/header.ejs") %> 
    <body>
        <%- include("partials/nav.ejs") %>
        <h1>Welcome, <%= name %>!</h1>
        <table class="table profile-table">
            <tr>
                <th>Name</th> <td><%=user[0].name%></td>
            </tr>
            <tr>
                <th>Email</th> <td><%=user[0].email%></td>
            </tr>
            <tr>
                <th>Phone</th> <td><%=user[0].phone%></td>
            </tr>
            <tr>
                <th>Zip</th> <td><%=user[0].zip%></td>
            </tr>
        </table>
        <form action="/logout">
            <button class="btn btn-primary">
                Logout
            </button>
        </form>
        <%- include("partials/footer.ejs") %>
    </body>
</html>